{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "rules": {
    "no-console": "off",
    "react/prefer-stateless-function": "off",
    "new-cap": "off"
  },
  "plugins": [
    "react"
  ],
  "env": {
    "jasmine": true
  }
}
